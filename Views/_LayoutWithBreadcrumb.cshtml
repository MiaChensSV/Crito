@using Umbraco.Cms.Web.Common.PublishedModels;
@inherits Umbraco.Cms.Web.Common.Views.UmbracoViewPage
@{
	Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta name="description" content="@Model.Value("pageDescription")" />
    <meta name="keywords" content="@Model.Value("pageKeywords")?.ToString()" />

    <title>@Model.Value("pageTitle")?.ToString() | Crito -Agency Service</title>
    <link asp-append-version="true" href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-aFq/bzH65dt+w6FI2ooMVUpc+21e0SRygnTpmBvdBgSdnuTN7QbdgL+OapgHtvPp" crossorigin="anonymous">
    <link rel="stylesheet" href="~/css/style.css" asp-append-version="true" />
</head>
<body>
    <div class="wrapper">
        <header>
            @await Html.PartialAsync("_NavSection")
            @await Html.PartialAsync("_BreadcrumbSection")
        </header>
        <main>
            @RenderBody()
        </main>
        
        @await Html.PartialAsync("_Subscribe")
        @await Html.PartialAsync("_Footer")
    </div>
    
    <script asp-append-version="true" src="https://kit.fontawesome.com/41c8b474e2.js" crossorigin="anonymous"></script>
    <script asp-append-version="true" src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha2/dist/js/bootstrap.bundle.min.js" integrity="sha384-qKXV1j0HvMUeCBQ+QVp7JcfGl760yU08IQ+GpUo5hlbpg51QRiuqHAJz8+BrxE/N" crossorigin="anonymous"></script>

</body>
</html>
